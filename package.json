{
    "name": "@genese/mapper",
    "version": "1.0.8",
    "author": {
        "name": "Gilles Fabre",
        "url": "https://github.com/gillesfabre34"
    },
    "scripts": {
        "build": "tsc",
        "build_and_test": "tsc && ts-node src/test-engine/start-tests.ts",
        "start": "npm run build && node dist/index.js",
        "publish_package": "npm run build && npm publish",
        "republish_package": "npm unpublish -f && npm run build && npm publish",
        "debug": "ts-node src/debug/project/start.ts",
        "test": "ts-node src/test-engine/start-tests-with-passed.ts",
        "test-old": "ts-node src/test-engine/start-tests-with-passed.old.ts",
        "test-all": "npm t && npm run test-type-checking",
        "test-fails": "ts-node src/test-engine/start-tests-without-passed.ts",
        "test-type-checking": "ts-node src/test-engine/test-type-checking.ts"
    },
    "devDependencies": {
        "@types/node": "^12.12.14",
        "chalk": "^4.1.0",
        "fs-extra": "^7.0.1",
        "ts-morph": "^9.1.0",
        "ts-node": "^7.0.1"
    },
    "keywords": [
        "genese",
        "mapper",
        "generic mapper",
        "typescript object mapper",
        "type typecsript objects"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "app-root-path": "^3.0.0",
        "reflect-metadata": "^0.1.13",
        "typescript": "^4.2.2"
    }
}
